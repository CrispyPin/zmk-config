			>;
		};
		layer_1 {
			bindings = <
&kp TAB	&kp X	&bt BT_SEL 0	&bt BT_NXT	&bt BT_CLR							&kp X	&kp X	&kp X		&kp X	&kp DEL
&kp N0	&kp N1	&kp N2			&kp N3		&kp N4								&kp N5	&kp N6	&kp N7		&kp N8	&kp N9
&trans	&kp X	&kp X			&kp X		&kp X								&kp X	&kp X	&kp COMMA	&kp DOT	&kp KP_ENTER
								&trans		&trans		&trans			&mo L0	&trans	&trans
